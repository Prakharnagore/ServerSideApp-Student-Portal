    <title>Student Marksheet</title>
  </head>
  <body>
    <div class="body00">
      <div class="welcome">
        <form action="/getstudentmarksheet" class="row" method="post">
          <div class="col-md-12">
            <label for="studentrollno">
              <h1>Enter Your Enrollment Number</h1>
            </label>
            <input
              type="search"
              class="form-control"
              name="studentrollno"
              id="studentrollno"
              placeholder="Enter Enrollment Number"
            />
          </div>
          <div class="buttons">
            <button class="btn btn-primary">View Result</button>
          </div>
        </form>
        <div class="buttons">
          <a href="/">
            <button class="btn btn-secondary">Back to Home Page</button></a
          >
        </div>
      </div>
    </div>
